<h1>Edit Data Mobil</h1>

<%= render 'validation_errors', object: @mobil %>

<%= form_for [:mod, @mobil] do |f| %>
	<p>
		<%= f.label :nama %><br>
		<%= f.text_field :nama %>
	</p>

	<p>
		<%= f.label :merk %><br>
		<%= f.text_field :merk %>
	</p>

	<p>
		<%= f.label :warna %>
		<%= f.text_field :warna %>
	</p>

	<p>
		<%= f.label :harga %>
		<%= f.text_field :harga %>
	</p>

	<p>
		<%= f.label :keterangan %>
		<%= f.check_box :keterangan %>
	</p>

	<p><%= f.submit %></p>
<% end %>
